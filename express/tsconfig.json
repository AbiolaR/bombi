{
    "compilerOptions": {
      "outDir": "./built",
      "allowJs": true,
      "target": "ES6",
      "module": "CommonJS",
      "esModuleInterop": true,
      "lib": [
        "ES2021.String"
      ]
    },
    "include": ["./src/**/*"],
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "strictNullChecks": true
}